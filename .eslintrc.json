{
  "env": {
    "jest": true,
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "standard", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "semi": "off",
    "semi-style": ["error", "last"],
    "react/prop-types": 0,
    "react/no-unescaped-entities": "off"
  }
}
